		"id":"logo",
		"type":"image_picker",
		"label":"Logo-img"
	},
	 {
        "type": "image_picker",
        "id": "search-icon",
        "label": "Search-Image"
      },
      {
        "type": "image_picker",
        "id": "user-icon",
        "label": "User-Image"
      },
      {
        "type": "image_picker",
        "id": "email-icon",
        "label": "Email-Image"
      },
      {
        "type": "image_picker",
        "id": "wishlist-icon",
        "label": "Heart-Image"
      },
	  {
        "type": "image_picker",
        "id": "menu-sideimg",
        "label": "Menu-SideImage"
      },
  		{
        "type": "image_picker",
        "id": "cancel",
        "label": "Cancel"
      },
  	  {
        "type": "image_picker",
        "id": "search",
        "label": "Search"
      },
	 {
        "type": "image_picker",
        "id": "navbar_icon",
        "label": "Navbar Ico"
      },
		 {
        "type": "image_picker",
        "id": "cart_bag",
        "label": "Cart Bag Icon"
      }
      ],
      "presets": [
          {
            "category": "Navbar",
            "name": "menu"
          }
        ] 
      }
